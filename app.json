{
  "expo": {
    "name": "cienecard",
    "slug": "cienecard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/utils/assets/icon.png",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.municieneguilla.cienecard",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to show your position on the map.",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/utils/assets/icon.png",
        "backgroundColor": "#124076"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "package": "com.municieneguilla.cienecard"
    },
    "web": {
      "favicon": "./src/utils/assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "2662db71-3bf6-41a9-9473-d2997b5ecbc0"
      }
    },
    "owner": "jdsalinas"
  }
}
